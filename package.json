{
  "name": "@aliceo2/aliceo2-gui",
  "version": "1.1.4",
  "description": "ALICE O2 Web UX framework",
  "author": "Adam Wegrzynek",
  "license": "GPL-3.0",
  "repository": "AliceO2Group/Gui",
  "homepage": "https://alice-o2.web.cern.ch",
  "scripts": {
    "test": "npm run eslint && npm run mocha && npm run nsp && npm run deps",
    "coverage": "./node_modules/.bin/nyc ./node_modules/mocha/bin/mocha --exit",
    "mocha": "./node_modules/.bin/mocha test/mocha-*",
    "travis": "./node_modules/.bin/mocha",
    "//": "./node_modules/.bin/phantomjs ./node_modules/qunit-phantomjs-runner/runner-list.js",
    "eslint": "./node_modules/.bin/eslint ./*.js http/*.js jwt/*.js public/*.js websocket/*.js zeromq/*.js test/*.js db/*.js log/*.js",
    "doc": "./node_modules/.bin/jsdoc2md --files ./*.js http/*.js jwt/*.js public/*.js websocket/*.js zeromq/*.js test/*.js db/*.js log/*.js > docs/API.md",
    "deps": "./node_modules/.bin/dependency-check ./package.json --unused --no-dev --ignore-module jquery",
    "nsp": "./node_modules/.bin/nsp check --warn-only",
    "postinstall": "mkdir -p public/libs; parent=`ls ..`; if [ \"$parent\" = aliceo2-gui ]; then cp ../../jquery/dist/jquery.js public/libs; else cp node_modules/jquery/dist/jquery.js public/libs; fi"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.1.0",
    "mustache": "^2.3.0",
    "mysql": "^2.15.0",
    "simple-oauth2": "^1.4.0",
    "winston": "^3.0.0-rc1",
    "ws": "^3.3.2",
    "zeromq": "^4.6.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "dependency-check": "^2.9.1",
    "eslint": "^4.12.0",
    "eslint-config-google": "^0.9.1",
    "jsdoc-to-markdown": "^3.0.2",
    "mocha": "^4.0.1",
    "nsp": "^3.1.0",
    "phantom": "^4.0.9",
    "qunit": "^2.4.1",
    "qunit-phantomjs-runner": "^2.3.1"
  },
  "main": "aliceo2-gui.js",
  "files": [
    "aliceo2-gui.js",
    "LICENSE",
    "README.md",
    "log",
    "config-default.json",
    "http",
    "jwt",
    "public",
    "websocket",
    "zeromq",
    "db"
  ]
}
