{
  "name": "@aliceo2/aliceo2-gui",
  "version": "1.0.22",
  "description": "ALICE O2 Web UX framework",
  "author": "Adam Wegrzynek",
  "license": "GPL-3.0",
  "repository": "AliceO2Group/Gui",
  "homepage": "https://alice-o2.web.cern.ch",
  "scripts": {
    "test": "npm run eslint && npm run mocha && npm run nsp",
    "coverage": "./node_modules/.bin/nyc ./node_modules/mocha/bin/mocha",
    "mocha": "./node_modules/.bin/mocha test/ --exit",
    "//": "./node_modules/.bin/phantomjs ./node_modules/qunit-phantomjs-runner/runner-list.js",
    "eslint": "./node_modules/.bin/eslint ./*.js http/*.js jwt/*.js public/*.js websocket/*.js zeromq/*.js test/*.js",
    "doc": "./node_modules/.bin/jsdoc2md --files ./*.js http/*.js jwt/*.js public/*.js websocket/*.js zeromq/*.js test/*.js > docs/API.md",
    "nsp": "./node_modules/.bin/nsp check --warn-only",
    "postinstall": "mkdir -p public/libs; parent=`ls ..`; if [ \"$parent\" = aliceo2-gui ]; then cp ../../jquery/dist/jquery.js public/libs; cp ../../jquery-ui-dist/jquery-ui.* public/libs; else cp node_modules/jquery/dist/jquery.js public/libs; cp node_modules/jquery-ui-dist/jquery-ui.* public/libs; fi"
  },
  "dependencies": {
    "apn": "^2.1.5",
    "body-parser": "^1.18.2",
    "chai-as-promised": "^7.1.1",
    "express": "^4.16.1",
    "helmet": "^3.8.2",
    "jquery": "^3.2.1",
    "jquery-ui-dist": "^1.12.1",
    "jsonwebtoken": "^8.0.1",
    "mustache": "^2.3.0",
    "mysql": "^2.14.1",
    "passport": "^0.4.0",
    "passport-saml": "^0.15.0",
    "simple-oauth2": "^1.3.0",
    "web-push": "^3.2.3",
    "winston": "^2.4.0",
    "ws": "^3.2.0",
    "yargs": "^9.0.1",
    "zeromq": "^4.6.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-google": "^0.9.1",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^4.0.0",
    "nsp": "^2.8.1",
    "nyc": "^11.2.1",
    "phantom": "^4.0.5",
    "qunit": "^1.0.0",
    "qunit-phantomjs-runner": "^2.3.0"
  },
  "main": "aliceo2-gui.js",
  "files": [
    "aliceo2-gui.js",
    "LICENSE",
    "README.md",
    "log.js",
    "config-default.json",
    "http",
    "jwt",
    "public",
    "websocket",
    "zeromq"
  ]
}
