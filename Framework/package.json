{
  "name": "@aliceo2/web-ui",
  "version": "1.8.5",
  "description": "ALICE O2 Web UX framework",
  "author": "Adam Wegrzynek",
  "license": "GPL-3.0",
  "repository": "AliceO2Group/WebUi",
  "homepage": "https://alice-o2.web.cern.ch",
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "coverage": "nyc ./node_modules/.bin/mocha Backend/test/ --exit",
    "mocha": "npm run mocha-frontend && npm run mocha-backend",
    "mocha-frontend": "./node_modules/.bin/mocha --exit ./Frontend/test/",
    "mocha-backend": "./node_modules/.bin/mocha --exit ./Backend/test/",
    "eslint": "./node_modules/.bin/eslint --config ../.eslintrc.js Backend/ Frontend/ docs/tutorial/*.js",
    "doc-frontend": "jsdoc2md --files \"./Frontend/**/*.js\" -p list -r list --member-index-format list --name-format --no-gfm -l js > ./docs/reference/frontend-api.md"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "helmet": "^3.15.1",
    "jsonwebtoken": "^8.5.0",
    "mysql": "^2.16.0",
    "simple-oauth2": "^2.2.1",
    "winston": "^3.2.1",
    "ws": "^6.1.4"
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-config-google": "^0.12.0",
    "mocha": "^6.1.4",
    "puppeteer": "^1.12.2"
  },
  "main": "Backend/index.js"
}
